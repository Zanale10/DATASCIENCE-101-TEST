      "cell_type": "code",
      "source": [
        "pca = PCA(n_components=0.95)  # keep 95% variance\n",
        "pipeline = Pipeline(steps=[\n",
        "    ('preprocessor', preprocessor),\n",
        "    ('pca', pca)\n",
        "])\n"
